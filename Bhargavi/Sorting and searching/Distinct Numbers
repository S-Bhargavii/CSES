#include <iostream>
#include <algorithm>
using namespace std;
#define ll long long
#include <set>
int main()
{
    ll n;
    cin >> n;
    ll k;
    set<ll> set1;
    // set is by default unordered
    for (ll i = 0; i < n; i++)
    {
        cin >> k;
        set1.insert(k);
    }
    cout << set1.size();
}
